<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onetouch.dao.PostDao">
	<select id="selectPostList" resultType="PostVo">
		select *from post
	</select>
	
	<select id="selectPostCategoryList" resultType="PostVo" parameterType="String">
		select *from post where post_category = #{post_category}
	</select>
	
	
</mapper>